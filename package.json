{
  "name": "ionic-tooltips",
  "version": "2.0.0",
  "description": "Tooltips module for Ionic apps",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf aot dist && ngc",
    "postversion": "npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postchangelog": "git commit -am \"chore(): update changelog\"",
    "shipit": "npm run lint && npm run build && npm publish",
    "pretest": "rm -rf dist && tsc",
    "test": "karma start karma.conf.js --single-run",
    "pretest:watch": "npm run pretest",
    "test:watch": "concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "lint": "tslint \"src/**/*.ts\""
  },
  "files": [
    "dist"
  ],
  "author": "Ibby Hadeed <ibby@zyra.ca>",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "4.1.3",
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/platform-server": "4.1.3",
    "@types/jasmine": "^2.5.54",
    "@types/node": "^8.0.28",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "codelyzer": "^3.1.2",
    "concurrently": "^3.5.0",
    "conventional-changelog-cli": "^1.3.3",
    "core-js": "^2.5.1",
    "ionic-angular": "3.6.1",
    "ionicons": "3.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.2.0",
    "rxjs": "5.4.1",
    "systemjs": "^0.20.18",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "2.3.4",
    "zone.js": "0.8.12"
